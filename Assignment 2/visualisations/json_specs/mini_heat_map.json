{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "url": "./visualisations/datasets/melb_property_data.csv"
    },
    "description": "A scatterplot showing body mass and flipper lengths of penguins.",
    "mark": "rect",
    "width": 500,
    "height": 500,
    "transform": [
        {
            "filter": {
                "and": [
                    {
                        "field": "Bedroom2",
                        "valid": true,
                        "lte": 7
                    },
                    {
                        "field": "Bathroom",
                        "valid": true,
                        "lte": 5
                    }
                ]
            }
        }
    ],
    "encoding": {
        "x": {
            "bin": {
                "maxbins": 5
            },
            "field": "Bathroom",
            "type": "quantitative"
        },
        "y": {
            "bin": {
                "maxbins": 5
            },
            "field": "Bedroom2",
            "type": "quantitative"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "config": {
        "view": {
            "stroke": "transparent"
        }
    }
}